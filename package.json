{
  "name": "postry-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --no-color",
    "test:coverage": "vitest --no-color --coverage",
    "test:e2e": "cross-env DOTENV_CONFIG_PATH=.env playwright test > test.log 2>&1",
    "test:e2e:setup": "cross-env DOTENV_CONFIG_PATH=.env playwright test e2e/auth.setup.chromium.ts e2e/auth.setup.firefox.ts e2e/auth.setup.webkit.ts",
    "test:e2e:one": "cross-env DOTENV_CONFIG_PATH=.env playwright test -g",
    "build:e2e": "cross-env NEXT_PUBLIC_QUIZ_USE_MOCK=true next build",
    "db:backup": "node scripts/backup-db.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.0",
    "clsx": "latest",
    "dotenv": "^17.2.3",
    "lucide-react": "latest",
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.0.0",
    "cross-env": "^10.1.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.17"
  }
}
